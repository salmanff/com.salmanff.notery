{
  "identifier": "com.salmanff.notery",
  "display_name":"Notery",
  "version": "0.0.400",
  "description": "A simple (somewhat outdated) note taking app",
  "app_url": "https://github.com/salmanff/com.salmanff.notery",
  "manifest_url": "https://github.com/salmanff/com.salmanff.notery/blob/main/manifest.json",

	"meta": {
		"app_name":"com.salmanff.notery",
		"app_display_name":"Notery",
		"app_version": "0.06 2020",
		"only_use_collections_listed":false
	},

	"pages": {
		"index": {
			"page_title":"Notery",
			"html_file":"index.html",
			"css_files": ["static/bootstrap.css", "static/font-awesome.min.css", "static/summernote.css","notery_main.css"],
			"script_files": [ "jquery-3.1.0.js", "bootstrap.js" , "jlos-frozen.js", "summernotery.js","FileSaver.js","notery.js","sjcl.js"]
		}
	},


	"app_tables": {
		"posts": {
			"strictly_Adhere_To_schema":false,
			"field_names":{
				"created_locally": {
					"description":"Date Created (device time)",
					"type": "date"
				},
				"fj_modified_locally": {
					"description":"Date Modified (device time)",
					"type": "date"
				},
				"fj_deleted": {
					"description":"deleted",
					"type": "bool"
				}
			}
		}
	},

	"permissions": {
	}

}
